@model Team1_DynamicForms.Models.WorkFlowCreateIndexViewModel
@{
    ViewBag.Title = "CreateIndex";
}

<h2>Choose which form to attach the new workflow to</h2>
@using (Html.BeginForm())
{
    @Html.DropDownListFor(model => model.SelectedIndex, Model.Forms);
    @Html.AntiForgeryToken()
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
}